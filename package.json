{
  "name": "outline",
  "private": true,
  "license": "AGPL-3.0-only",
  "description": "Open source wiki and knowledge base for growing teams",
  "homepage": "https://www.getoutline.com",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ],
  "scripts": {
    "clean": "rm -rf build",
    "build": "yarn build:server && yarn build:app",
    "build:app": "vite build",
    "build:server": "tsc --project ./server/tsconfig.json",
    "build:server:watch": "tsc --project ./server/tsconfig.json --watch",
    "dev": "nodemon",
    "dev:app": "vite",
    "dev:server": "NODE_OPTIONS=\"--enable-source-maps\" node --inspect=9229 -r dotenv/config build/server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "i18n": "i18next",
    "i18n:generate": "i18next 'app/**/*.{ts,tsx}'",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "start": "NODE_OPTIONS=\"--enable-source-maps\" node -r dotenv/config build/server/index.js",
    "start:metrics": "NODE_OPTIONS=\"--enable-source-maps\" node -r dotenv/config build/server/metrics.js",
    "start:web": "NODE_OPTIONS=\"--enable-source-maps\" node -r dotenv/config build/server/web.js",
    "start:collaboration": "NODE_OPTIONS=\"--enable-source-maps\" node -r dotenv/config build/server/collaboration.js",
    "start:worker": "NODE_OPTIONS=\"--enable-source-maps\" node -r dotenv/config build/server/queues/worker.js",
    "start:websockets": "NODE_OPTIONS=\"--enable-source-maps\" node -r dotenv/config build/server/websockets/index.js",
    "db:create": "node -r dotenv/config ./server/scripts/create-db.js",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "db:setup": "yarn db:create && yarn db:migrate && yarn db:seed",
    "db:structure:dump": "node -r dotenv/config ./server/scripts/db-structure-dump.js",
    "db:backup": "node -r dotenv/config ./server/scripts/db-backup.js",
    "db:restore": "node -r dotenv/config ./server/scripts/db-restore.js",
    "db:truncate": "node -r dotenv/config ./server/scripts/db-truncate.js",
    "db:reset": "node -r dotenv/config ./server/scripts/db-reset.js",
    "db:recreate": "node -r dotenv/config ./server/scripts/db-recreate.js",
    "db:version": "sequelize db:version",
    "sequelize": "sequelize",
    "sequelize:migrate:make": "sequelize migration:generate",
    "sequelize:seed:make": "sequelize seed:generate",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      ".vscode",
      "build",
      "public",
      "app"
    ],
    "delay": 1000,
    "ext": "js,jsx,ts,tsx,json",
    "signal": "SIGTERM",
    "exec": "yarn build:server && yarn dev:server"
  },
